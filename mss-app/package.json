{
  "name": "mss-app",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": ">= 24"
  },
  "type": "module",
  "imports": {
    "#/*": [
      "./src/*"
    ],
    "#composables/*": [
      "./src/composables/*"
    ],
    "#views/*": [
      "./src/views/*"
    ],
    "#utils/*": [
      "./src/utils/*"
    ],
    "#conference/*": [
      "./src/components/conference/*"
    ],
    "#noTv/*": [
      "./src/components/noTVGames/*"
    ],
    "#season/*": [
      "./src/components/season/*"
    ],
    "#shared/*": [
      "./src/components/shared/*"
    ],
    "#weekly/*": [
      "./src/components/weekly/*"
    ],
    "#text/*": [
      "./src/components/weeklyText/*"
    ],
    "#data/*": [
      "./src/staticData/*"
    ]
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "vite",
    "build": "pnpm type-check && pnpm build-only",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint .",
    "lintFix": "eslint . --fix",
    "format": "prettier --write ./",
    "copy-webp": "node ./scripts/copy-webp.mjs"
  },
  "dependencies": {
    "@apollo/client": "^4.1.2",
    "@unhead/vue": "^2.1.2",
    "@unpic/vue": "^1.0.0",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vue/apollo-composable": "5.0.0-alpha.1",
    "@vueuse/core": "^14.1.0",
    "graphql": "^16.12.0",
    "luxon": "^3.7.2",
    "vite": "^7.3.1",
    "vite-plugin-pwa": "^1.2.0",
    "vue": "^3.5.27",
    "vue-dompurify-html": "^5.3.0",
    "vue-router": "^5.0.1"
  },
  "devDependencies": {
    "@apollo/client-devtools-vscode": "^4.24.3",
    "@rushstack/eslint-patch": "^1.15.0",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "eslint-plugin-vue": "^10.7.0",
    "eslint-plugin-vuejs-accessibility": "^2.4.1",
    "lightningcss": "^1.31.1",
    "rollup-plugin-visualizer": "^6.0.5",
    "sharp": "^0.34.5",
    "terser": "^5.46.0",
    "vite-plugin-vue-devtools": "^8.0.5",
    "vue-tsc": "^3.2.4",
    "workbox-build": "^7.4.0",
    "workbox-window": "^7.4.0"
  },
  "graphql": {
    "schema": "./schema.graphql",
    "documents": "./src/**/*.{graphql,js,mts,jsx,tsx}"
  },
  "packageManager": "pnpm@10.28.2"
}
