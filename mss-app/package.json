{
  "name": "mss-app",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": ">= 24"
  },
  "type": "module",
  "imports": {
    "#/*": [
      "./src/*"
    ],
    "#composables/*": [
      "./src/composables/*"
    ],
    "#views/*": [
      "./src/views/*"
    ],
    "#utils/*": [
      "./src/utils/*"
    ],
    "#conference/*": [
      "./src/components/conference/*"
    ],
    "#noTv/*": [
      "./src/components/noTVGames/*"
    ],
    "#season/*": [
      "./src/components/season/*"
    ],
    "#shared/*": [
      "./src/components/shared/*"
    ],
    "#weekly/*": [
      "./src/components/weekly/*"
    ],
    "#text/*": [
      "./src/components/weeklyText/*"
    ],
    "#data/*": [
      "./src/staticData/*"
    ]
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "vite",
    "build": "pnpm type-check && pnpm build-only",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint .",
    "lintFix": "eslint . --fix",
    "format": "prettier --write ./"
  },
  "dependencies": {
    "@apollo/client": "^4.0.7",
    "@unhead/vue": "^2.0.19",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vueuse/core": "^13.9.0",
    "graphql": "^16.11.0",
    "luxon": "^3.7.2",
    "vite": "^7.1.11",
    "vite-plugin-pwa": "^1.1.0",
    "vue": "^3.5.22",
    "vue-dompurify-html": "^5.3.0",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@apollo/client-devtools-vscode": "^4.21.9",
    "@rushstack/eslint-patch": "^1.14.0",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "eslint-plugin-vue": "^10.5.1",
    "eslint-plugin-vuejs-accessibility": "^2.4.1",
    "lightningcss": "^1.30.2",
    "rollup-plugin-visualizer": "^6.0.5",
    "terser": "^5.44.0",
    "vite-plugin-vue-devtools": "^8.0.2",
    "vue-tsc": "^3.1.1"
  },
  "graphql": {
    "schema": "./schema.graphql",
    "documents": "./src/**/*.{graphql,js,mts,jsx,tsx}"
  },
  "packageManager": "pnpm@10.18.2"
}
