    "@vueuse/core": "^13.5.0",
    "graphql": "^16.11.0",
    "luxon": "^3.7.1",
    "vite": "^6.3.5",
    "vite-plugin-pwa": "^1.0.1",
    "vue": "^3.5.17",
    "vue-dompurify-html": "^5.3.0",
    "vue-router": "^4.5.1"
