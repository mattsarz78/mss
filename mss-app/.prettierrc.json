{
  "arrowParens": "always",
  "bracketSpacing": true,
  "endOfLine": "lf",
  "insertPragma": false,
  "htmlWhitespaceSensitivity": "css",
  "quoteProps": "as-needed",
  "proseWrap": "preserve",
  "requirePragma": false,
  "semi": true,
