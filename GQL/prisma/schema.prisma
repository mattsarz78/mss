generator client {
  provider = "prisma-client-js"
  output   = "../__generated__/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model football {
  id             BigInt    @id @default(autoincrement())
  season         String?
  gametitle      String?
  visitingteam   String?
  hometeam       String?
  location       String?
  network        String?
  networkjpg     String?
  coveragenotes  String?
  ppv            String?
  time           DateTime? @default(dbgenerated("'9999-12-31 00:00:00'::timestamp without time zone")) @db.Timestamp(6)
  week           Int?      @db.SmallInt
  listorder      Int?      @db.SmallInt
  mediaindicator String?   @default("N")
  conference     String?
  fcs            String?
  tvtype         String?
  timewithoffset DateTime? @default(dbgenerated("'9999-12-31 00:00:00+00'::timestamp with time zone")) @db.Timestamp(6)
}

model basketball {
  id             BigInt    @id @default(autoincrement())
  season         String?
  gametitle      String?
  visitingteam   String?
  hometeam       String?
  location       String?
  network        String?
  networkjpg     String?
  coveragenotes  String?
  ppv            String?
  time           DateTime? @default(dbgenerated("'9999-12-31 00:00:00'::timestamp without time zone")) @db.Timestamp(6)
  week           Int?      @db.SmallInt
  listorder      Int?      @db.SmallInt
  mediaindicator String?   @default("N")
  conference     String?
  fcs            String?
  tvtype         String?
  timewithoffset DateTime? @default(dbgenerated("'9999-12-31 00:00:00+00'::timestamp with time zone")) @db.Timestamp(6)
}

model availabletv {
  season     String
  conference String
  week       Int     @db.SmallInt
  tvoptions  String?

  @@id([season, conference, week])
}

model rsnlist {
  id           BigInt  @id
  season       String?
  gametitle    String?
  visitingteam String?
  hometeam     String?
  location     String?
  keyvalue     String?
}

model weeklydates {
  season        String
  week          Int       @db.SmallInt
  startdate     DateTime? @db.Timestamp(6)
  enddate       DateTime? @db.Timestamp(6)
  postseasonind String?

  @@id([season, week])
}
